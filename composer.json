{
    "name": "guillermoandrae/andrae-ui",
    "description": "andrae UI",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "guzzlehttp/guzzle": "^6.3",
        "icanboogie/inflector": "^1.4",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^3.0",
        "phing/phing": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        },
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
